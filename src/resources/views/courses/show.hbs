<div class="mt-4">
  <div class="row g-3">
    <div class="col-2">
      <a href="#" class="btn btn-primary">Learn now</a>
    </div>
    <div class="col-9">
      {{#with course}}
      <div class="card">
        <img src="{{image}}" height="500" />
        <div class="card-body">
          <h5 class="card-title">{{name}}</h5>
          <p class="card-text">{{description}}</p>
        </div>
      </div>
      
      {{/with}}
    </div>
  </div>
</div>