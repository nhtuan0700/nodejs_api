<div class="mt-4">
  <div class="row g-3">
    {{#each courses}}
    <div class="col-sm-6 col-md-4">
      <div class="card h-100">
        <img src="{{image}}" />
        <div class="card-body">
          <h5 class="card-title">{{title}}</h5>
        </div>
        <p class="card-text">{{this.description}}</p>
        <a href="#" class="btn btn-primary">Go</a>
      </div>
    </div>
    {{/each}}
  </div>
</div>